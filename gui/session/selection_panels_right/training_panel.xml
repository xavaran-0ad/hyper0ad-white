<?xml version="1.0" encoding="UTF-8"?>
<object name="unitTrainingPanel"
	size="6 6 120% 120%"
>
	<object>
		<repeat count="50">
			<object name="unitTrainingButton[n]" hidden="true" style="iconButton" type="button" size="0 0 45 45" tooltip_style="sessionToolTipBottom">
				<object name="unitTrainingIcon[n]" type="image" ghost="true" size="3 3 47 47"/>
				<object name="unitTrainingCount[n]" ghost="true" style="groupIconsText" type="text" z="20"/>
				<action on="MouseWheelUp">
					OnTrainMouseWheel(1);
				</action>
				<action on="MouseWheelDown">
					OnTrainMouseWheel(-1);
				</action>
			</object>
		</repeat>
	</object>
</object>
